{
  "Nats": {
    "Host": "nats.ubk3s", //overridden by env var Nats__Host in K8s
    "Port": 4222,
    "Stream": "remoteactions",
    "Subjects": [
      "remoteactions.*"
    ],
    "Subject": "remoteactions.new",
    "Consumer": "remote_processor"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "NATS.Client.Core": "Warning"
    }
  },
  "AllowedHosts": "*"
}
